#include <iostream>
#include <bits/stdc++.h>
#include<string>
using namespace std;
int main() {
    string s,a;
    int i, temp,c = 0;
    cin>>s;
 
    sort(s.begin(),s.end());
   // cout << s <<endl;
    for( i= 0 ; i < s.length() ; i++)
    {
            if (s[i] != s[i + 1])
            {
                c++;
 
            }
 
        //c++;
    }
 
    if(c%2 == 0)
    {
        cout<< "CHAT WITH HER!";
    }
    else
    {
        cout<<"IGNORE HIM!";
    }
    return 0;
}
