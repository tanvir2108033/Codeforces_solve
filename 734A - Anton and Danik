#include<iostream>
using namespace std;
int main()
{
    int n,  a = 0 , d = 0;
    cin>>n;
    char arr[n];
    for(int i = 0; i < n ; i++)
    {
        cin >> arr[i];
    }
    for(int i = 0; i < n ; i++)
    {
        if(arr[i] == 'A')
        {
            a++;
        }
        else if(arr[i] == 'D')
        {
            d++;
        }
    }
    if (a>d)
    {
        cout<<"Anton"<<endl;
    }
    else if ( a < d)
    {
        cout << "Danik"<<endl;
    }
    else if(a == d)
    {
        cout<<"Friendship"<<endl;
    }
}
