#include <iostream>
#include<string>
using namespace std;
 
int
main ()
{
  string s;
  int i, n, a = 0, b = 0;
  cin >> n;
  for (i = 1; i <= n; i++)
	{
	  cin >> s;
	  for (int j = 0; j < 5; j++)
		{
		  if (s[j] == 'A')
			{
			  a++;
			}
		  else if(s[j] == 'B')
			{
			  b++;
			}
		}
		if( a > b )
		{
		    cout<<"A"<<endl;
		}
		else
		{
		    cout<<"B"<<endl;
		}
		a = 0;
		b =0;
	}
 
  return 0;
}
