
#include <iostream>
using namespace std;
 
int main() {
    
    int i, n, h, p , cnt = 0;
    cin>>n>>h;
    
    for( i = 1 ; i <= n ; i++ )
    {
        cin>>p;
        if( p <= h )
        {
            cnt++;
        }
        else
        {
            cnt = cnt + 2;
        }
    }
    cout<<cnt<<endl;
    
    return 0;
}
